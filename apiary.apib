                "object":{
                    "href": "/projects/{projectId}/resourceplans/{dateGrouping}/{groupKey}/{resourceTypeId}/",
                    "url": "http://devppm.daptiv.com/_api/projects/{projectId}/resourceplans/{dateGrouping}/{resourceTypeId}",
                    "schemaUrl" : "...",
                    "data" : {
                        "resourceType" : "resourceType/{resourceTypeId}",
